<VirtualHost *:80>
  ServerName gottung.net
  ServerAlias www.gottung.net
  ServerAdmin webmaster@nlsmith.com

  DocumentRoot /home/smith/Sites/gottung.net
  <Directory /home/smith/Sites/gottung.net/>
    AllowOverride All
  </Directory>

	ServerSignature Off

	# Possible values include: debug, info, notice, warn, error, crit,
	# alert, emerg.
	LogLevel warn
  ErrorLog /var/log/apache2/gottung.net.error.log
  CustomLog /var/log/apache2/gottung.net.access.log combined
</VirtualHost>
